{
  "name": "kcy-ecosystem",
  "version": "1.0.0",
  "description": "KCY Ecosystem - Token, Multi-Sig, and Chat unified structure",
  "scripts": {
    "test": "npm run test:token && npm run test:multisig && npm run test:chat",
    "test:token": "hardhat test tests/token/*.js --network hardhat",
    "test:multisig": "hardhat test tests/multisig/*.js --network hardhat",
    "test:chat": "jest tests/chat --config jest.config.js",
    "test:token:verbose": "hardhat test tests/token/*.js --network hardhat --verbose",
    "test:multisig:verbose": "hardhat test tests/multisig/*.js --network hardhat --verbose",
    "deploy:token": "hardhat run private/token/scripts/deploy.js --network bscMainnet",
    "deploy:multisig": "hardhat run private/multisig/scripts/deploy-multisig.js --network bscMainnet",
    "admin:token:status": "hardhat run private/token/admin/scripts/01-check-status.js --network bscMainnet",
    "admin:token:pause": "hardhat run private/token/admin/scripts/02-pause-control.js --network bscMainnet",
    "start:chat": "node private/chat/server.js",
    "start:chat:dev": "nodemon private/chat/server.js"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "bsc",
    "token",
    "multisig",
    "chat",
    "crypto"
  ],
  "author": "KCY Ecosystem",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.0",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.0",
    "@nomicfoundation/hardhat-ledger": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "chai": "^4.2.0",
    "ethers": "^6.9.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "sqlite3": "^5.1.6",
    "web3": "^4.3.0",
    "ws": "^8.16.0"
  }
}
