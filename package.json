{
  "name": "kcy-ecosystem",
  "version": "1.0062",
  "description": "KCY Ecosystem - Token, Multi-Sig, and Chat unified structure",
  "workspaces": [
    "private/token",
    "private/multisig",
    "private/chat",
    "private/mobile-chat"
  ],
  "scripts": {
    "test": "npm run test:token && npm run test:multisig && npm run test:chat && npm run test:mobile",
    "test:token": "cd private/token && npm test",
    "test:multisig": "cd private/multisig && npm test",
    "test:chat": "cd private/chat && npm test",
    "test:mobile": "cd private/mobile-chat && npm test",
    "deploy:token": "cd private/token && npx hardhat run scripts/deploy.js --network bscMainnet",
    "deploy:multisig": "cd private/multisig && npx hardhat run scripts/deploy-multisig.js --network bscMainnet",
    "start:chat": "cd private/chat && node server.js",
    "start:chat:dev": "cd private/chat && nodemon server.js"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "bsc",
    "token",
    "multisig",
    "chat",
    "crypto"
  ],
  "author": "KCY Ecosystem",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
